<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >
<xacro:include filename="inertial_macros.xacro" />



    <joint name="arm_base_joint" type="continuous">
        <parent link="chassis"/>
        <child link="base_arm"/>
        <origin xyz="0.2 0 0.17" rpy="0 0 0.1"/>
        <axis xyz="0 0 1"/> 
    	<dynamics damping="2" friction="1"/>
    </joint>
    
    <joint name="arm_joint" type="revolute">
        <parent link="base_arm"/>
        <child link="arm_2"/>
        <origin xyz="0 0 0.04" rpy="0 0 0"/>
    	<axis xyz="0 1 0"/>
    	<limit effort="50" velocity="1" lower="-1.5" upper="1.6"/>
    	<dynamics damping="2" friction="1"/>
    </joint>
    
    <joint name="arm_joint_2" type="revolute">
        <parent link="arm_2"/>
        <child link="arm"/>
        <origin xyz="0 0.025 0.35" rpy="0 0 0"/>
    	<axis xyz="0 1 0"/>
    	<limit effort="50" velocity="1" lower="-0.5" upper="1.6"/>
    	<dynamics damping="1" friction="0.25"/>
    </joint>

    <joint name="arm_joint_3" type="revolute">
        <parent link="arm"/>
        <child link="arm_3"/>
        <origin xyz="0 -0.025 0.35" rpy="0 0 0"/>
    	<axis xyz="0 1 0"/>
    	<limit effort="50" velocity="1" lower="-1.6" upper="1.6"/>
    	<dynamics damping="0.1" friction="0.1"/>
    </joint>
    

    
    <joint name="finger1_joint" type="prismatic">
        <parent link="palm"/>
        <child link="finger1"/>
        <origin xyz="0.07 0 0" rpy="0 0 0"/>
        <axis xyz="-1 0 0"/> 
        <limit effort="50" velocity="1" lower="0" upper="0.06"/>
        <dynamics damping="1" friction="1"/>
    </joint>
    
        
    <joint name="finger2_joint" type="prismatic">
        <parent link="palm"/>
        <child link="finger2"/>
        <origin xyz="-0.07 0 0" rpy="0 0 0"/>
        <axis xyz="1 0 0"/> 
        <limit effort="50" velocity="1" lower="0" upper="0.06"/>
        <dynamics damping="1" friction="2" />
    </joint>
    
    <joint name="palm_joint" type="continuous">
        <parent link="arm_3"/>
        <child link="palm"/>
        <origin xyz="0 0 0.1" rpy="0 0 0.1"/>
        <axis xyz="0 0 1"/> 
        <dynamics damping="0.11" friction="0.01"/>
    </joint>
    
    
    
    <link name="arm">
        <visual>
            <origin xyz="0 0 0.1725" rpy="0 0 0"/>
            <geometry>
                <box size="0.03 0.03 0.4"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0.1725" rpy="0 0 0"/>
            <geometry>
                <box size="0.03 0.03 0.4"/>
            </geometry>
        </collision>
        <xacro:inertial_box mass="0.08" x="0.03" y="0.03" z="0.4">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>
    <gazebo reference="arm">
  	<material>Gazebo/White</material>
    </gazebo>
    
     <link name="arm_2">
        <visual>
            <origin xyz="0 0 0.1725" rpy="0 0 0"/>
            <geometry>
                <box size="0.03 0.03 0.4"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0.1725" rpy="0 0 0"/>
            <geometry>
                <box size="0.03 0.03 0.4"/>
            </geometry>
        </collision>
        <xacro:inertial_box mass="0.08" x="0.03" y="0.03" z="0.4">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>
    <gazebo reference="arm_2">
  	<material>Gazebo/White</material>
    </gazebo>

    <link name="arm_3">
        <visual>
            <origin xyz="0 0 0.044" rpy="0 0 0"/>
            <geometry>
                <box size="0.03 0.03 0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0.044" rpy="0 0 0"/>
            <geometry>
                <box size="0.03 0.03 0.1"/>
            </geometry>
        </collision>
        <xacro:inertial_box mass="0.08" x="0.03" y="0.03" z="0.1">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>
    <gazebo reference="arm_3">
  	<material>Gazebo/White</material>
    </gazebo>
    
    <link name="base_arm">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.05"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0.05" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>
    <gazebo reference="base_arm">
  	<material>Gazebo/White</material>
    </gazebo>
    
    
 
    <link name="palm">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.14 0.025 0.02"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.14 0.025 0.02"/>
            </geometry>
        </collision>
        <xacro:inertial_box mass="0.07" x="0.1" y="0.025" z="0.02">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>
    <gazebo reference="palm">
  	<material>Gazebo/Blue</material>
    </gazebo>

    
    <link name="finger1">
        <visual>
            <origin xyz="0.01 0 0.05" rpy="0 0 0"/>
            <geometry>
                <box size="0.03 0.02 0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="0.01 0 0.05" rpy="0 0 0"/>
            <geometry>
                <box size="0.03 0.02 0.1"/>
            </geometry>
        </collision>
        <xacro:inertial_box mass="0.05" x="0.03" y="0.02" z="0.1">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>
    <gazebo reference="finger1">
  	<material>Gazebo/Red</material>
  	<selfCollide>false</selfCollide>
    </gazebo>
    


    <link name="finger2">
        <visual>
            <origin xyz="-0.01 0 0.05" rpy="0 0 0"/>
            <geometry>
                <box size="0.03 0.02 0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="-0.01 0 0.05" rpy="0 0 0"/>
            <geometry>
                <box size="0.03 0.02 0.1"/>
            </geometry>
        </collision>
        <xacro:inertial_box mass="0.05" x="0.03" y="0.02" z="0.1">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>
    <gazebo reference="finger2">
  	<material>Gazebo/Red</material>
  	<selfCollide>false</selfCollide>
    </gazebo>
 
</robot>
